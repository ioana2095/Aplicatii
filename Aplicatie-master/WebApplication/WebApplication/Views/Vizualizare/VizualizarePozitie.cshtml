@model WebApplication.Models.VizualizarePozitie

@{
    ViewBag.Title = "VizualizarePozitie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <style>
        div.container {
            width: 100%;
            border: 0px solid gray;
        }

        header, footer {
            padding: 1em;
            color: white;
            background-color: black;
            clear: left;
            text-align: center;
        }

        nav {
            float: left;
            max-width: 160px;
            margin: 0;
            padding: 1em;
        }

            nav ul {
                list-style-type: none;
                padding: 0;
            }

                nav ul a {
                    text-decoration: none;
                }

        article {
            margin-left: 170px;
            border-left: 1px solid gray;
            padding: 1em;
            overflow: hidden;
        }
    </style>
</head>
    <header>
        <h2>Vizualizare Pozitie</h2>
    </header>
    <div>
        <h4>VizualizarePozitie</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.Titlu)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Titlu)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Raft)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Raft)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Rand)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Rand)
            </dd>
            <div class="panel panel-default">
                <div class="panel-heading">Pozitia</div>
                <div class="panel-body">
                    <li class="list-group-item">
                        @foreach (var item in Model.Poz)
            {

                            <ul class="list-group">
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item)
                                    </td>
                                </tr>
                            </ul>
                        }
                    </li>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Identificare</div>
                <div class="panel-body">
                    <li class="list-group-item">
                        @foreach (var item in Model.Identificare)
            {
                            <ul class="list-group">
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item)
                                    </td>
                                </tr>
                            </ul>
                        }
                    </li>
                </div>
            </div>
        </dl>

    </div>
    <p>
        @Html.ActionLink("Back to List", "VizualizareCarti", "Vizualizare", new { Admin = 0 },null)
    </p>
